{
  "name": "pg-quasar-tailwindcss",
  "version": "0.0.0",
  "description": "A Quasar Project",
  "productName": "QuCommerce",
  "author": "Pinegrow <vuedesigner@pinegrow.com>",
  "homepage": "https://pg-quasar-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-quasar-tailwindcss.git"
  },
  "scripts": {
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "dev:electron": "quasar dev -m electron",
    "dev:ios": "quasar dev -m ios",
    "dev:android": "quasar dev -m android",
    "now": "npm run build && npm run preview",
    "lighthouse:dev": "npx unlighthouse --no-cache --site http://localhost:9000",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-quasar-tailwindcss.netlify.app",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "build:electron": "quasar build -m electron",
    "build:ios": "quasar build -m ios",
    "build:android": "quasar build -m android",
    "preview": "quasar serve dist/spa --history",
    "icons": "icongenie g -i icon.png --theme-color 00B4DB --skip-trim",
    "vue-devtools": "vue-devtools",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts ./ --ignore-path .gitignore",
    "format": "prettier --write \"**/*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts,html,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "@unhead/vue": "^1.8.10",
    "@vueuse/core": "^10.7.2",
    "lite-youtube-embed": "^0.3.0",
    "ofetch": "^1.3.3",
    "pinia": "^2.0.11",
    "quasar": "^2.14.1",
    "vee-validate": "^4.12.5",
    "vue": "^3.2.40",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.176",
    "@pinegrow/quasar-module": "^3.0.60",
    "@pinegrow/tailwindcss-plugin": "^3.0.29",
    "@quasar/app-vite": "^1.7.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.13",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@unocss/preset-icons": "^0.58.4",
    "@vue/devtools": "^6.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "unocss": "^0.58.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0"
  },
  "engines": {
    "node": "^20 || ^18 || ^16",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "web-types": "web-types.json"
}
