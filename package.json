{
  "name": "pg-quasar-tailwindcss",
  "version": "0.0.0",
  "type": "module",
  "description": "A Quasar Project",
  "productName": "QuCommerce",
  "author": "Pinegrow <vuedesigner@pinegrow.com>",
  "homepage": "https://pg-quasar-tailwindcss.netlify.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/pinegrow/pg-quasar-tailwindcss.git"
  },
  "scripts": {
    "dev": "quasar dev",
    "dev:ssr": "quasar dev -m ssr",
    "dev:electron": "quasar dev -m electron",
    "dev:ios": "quasar dev -m ios",
    "dev:android": "quasar dev -m android",
    "postinstall": "quasar prepare",
    "now": "npm run build && npm run preview",
    "lighthouse": "npx unlighthouse --no-cache --site https://pg-quasar-tailwindcss.netlify.app",
    "build": "quasar build",
    "build:ssr": "quasar build -m ssr",
    "build:electron": "quasar build -m electron",
    "build:ios": "quasar build -m ios",
    "build:android": "quasar build -m android",
    "preview": "quasar serve dist/spa --history",
    "icons": "icongenie g -i icon.png --theme-color 00B4DB --skip-trim",
    "lint": "npm run lint:fix && npm run format",
    "lint:fix": "eslint --fix ./",
    "format": "prettier --write \"**/*.{html,vue,svelte,astro,js,jsx,cjs,mjs,ts,tsx,cts,mts,css,md,mdx,json}\" !public --ignore-path .gitignore"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.15",
    "@unhead/schema": "^1.11.15",
    "@unhead/schema-org": "^1.11.15",
    "@unhead/vue": "^1.11.15",
    "@vueuse/core": "^12.4.0",
    "axios": "^1.7.9",
    "lite-youtube-embed": "^0.3.3",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "quasar": "^2.17.6",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@iconify/json": "^2.2.294",
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@pinegrow/quasar-module": "^3.0.69",
    "@pinegrow/tailwindcss-plugin": "^3.0.35",
    "@quasar/app-vite": "^2.0.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^22.10.5",
    "@unocss/preset-icons": "^65.4.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-shiki": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "unocss": "^65.4.0",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
    "unplugin-vue-markdown": "^28.0.0",
    "unplugin-vue-router": "^0.10.9",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "~2.1.6"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  },
  "web-types": "web-types.json"
}
